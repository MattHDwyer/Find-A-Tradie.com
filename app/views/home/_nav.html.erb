<%= render 'modal' %>
<% if !current_user %>
  <button type="button" class="btn btn-secondary" data-toggle="modal" data-target="#modalForm">
    List your Profile
  </button>
<% else %>
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/dashboards">Your Profile</a></li>
      <li class="breadcrumb-item"> <%= link_to "Sign out", destroy_user_session_path, {method: :delete} %></li>
    </ol>
  </nav>
<% end %>
