<%= render 'modal' %>
<% if !current_user %>
  <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalForm">
    List your Profile
  </button>
<% else %>
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"> <%= link_to "Home", "/", {method: :get} %></li>
      <li class="breadcrumb-item"> <%= link_to "Your Profile", "/dashboards", {method: :get} %></li>
      <li class="breadcrumb-item"> <%= link_to "Sign out", destroy_user_session_path, {method: :delete} %></li>
    </ol>
  </nav>
<% end %>
